    <main class="main-content">
        <div class="container">
            <section class="executive-section">
                <div class="executive-grid">
                    
                    <div class="executive-card">
                        <img src="https://via.placeholder.com/300x300.png?text=Photo" alt="Profile photo of Dr. Manorama Devi">
                        <div class="card-body">
                            <h3 class="name">Dr. Manorama Devi</h3>
                            <p class="designation">President</p>
                        </div>
                    </div>

                    <div class="executive-card">
                        <img src="https://via.placeholder.com/300x300.png?text=Photo" alt="Profile photo of Mr. Debasish Pattnaik">
                        <div class="card-body">
                            <h3 class="name">Mr. Debasish Pattnaik</h3>
                            <p class="designation">Secretary</p>
                        </div>
                    </div>

                    <div class="executive-card">
                        <img src="https://via.placeholder.com/300x300.png?text=Photo" alt="Profile photo of Mr. Sadananda Uthansingh">
                        <div class="card-body">
                            <h3 class="name">Mr. Sadananda Uthansingh</h3>
                            <p class="designation">Donor</p>
                        </div>
                    </div>
                    
                    <div class="executive-card">
                        <img src="https://via.placeholder.com/300x300.png?text=Photo" alt="Profile photo of Mr. Bhima Sabar">
                        <div class="card-body">
                            <h3 class="name">Mr. Bhima Sabar</h3>
