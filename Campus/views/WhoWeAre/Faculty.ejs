<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>KASH Teaching Staff</title>
    <link rel="stylesheet" href="/css/WhoWeAre/faculty.css">
</head>
<body>

    <header>
        <h1>KASH TEAM</h1>
        <h2>KASH TEACHING STAFF</h2>
    </header>

    <!-- Teaching Staff Profiles -->
    <div class="profile-card">
        <img src="https://via.placeholder.com/250x300.png?text=Profile+Image" alt="Profile Photo">
        <div class="info">
            <h3>NAME</h3>
            <p>Mr. Sudhansu Sekhar Panda</p>
            <h3>Designation</h3>
            <p>Principal</p>
        </div>
    </div>

    <!-- Repeat the above block for each teaching staff member -->
    <!-- Example: -->
    <div class="profile-card">
        <img src="https://via.placeholder.com/250x300.png?text=Profile+Image" alt="Profile Photo">
        <div class="info">
            <h3>NAME</h3>
            <p>Mrs. Example Name</p>
            <h3>Designation</h3>
            <p>Lecturer</p>
        </div>
    </div>

    <!-- Subheading Section -->
    <section class="subheading">
        <h2>KASH ADMINISTRATION AND SUPPORT STAFF</h2>
    </section>

    <!-- Administration Profiles -->
    <div class="profile-card">
        <img src="https://via.placeholder.com/250x300.png?text=Profile+Image" alt="Profile Photo">
        <div class="info">
            <h3>NAME</h3>
            <p>Mr. John Doe</p>
            <h3>Designation</h3>
            <p>Admin Head</p>
        </div>
    </div>

    <!-- Repeat as needed -->
    <div class="profile-card">
        <img src="https://via.placeholder.com/250x300.png?text=Profile+Image" alt="Profile Photo">
        <div class="info">
            <h3>NAME</h3>
            <p>Ms. Jane Doe</p>
            <h3>Designation</h3>
            <p>Office Assistant</p>
        </div>
    </div>

</body>
</html>
